{
  "name": "toynet-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "axios": "^0.21.1",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "immer": "^7.0.7",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.2",
    "react-query": "^2.5.7-tsnext.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "use-immer": "^0.4.1"
  },
  "scripts": {
    "check-types": "tsc",
    "style:check": "eslint --ext .js --ext .jsx --ext tsx --ext ts ./src --cache",
    "style:fix": "eslint --ext .js --ext .jsx --ext tsx --ext ts ./src --quiet --fix --cache",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "serviceWorker.js",
      "*.test.js*"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-magic-numbers": [
        2,
        {
          "ignore": [
            -1,
            0,
            1,
            2
          ],
          "enforceConst": true,
          "detectObjects": true
        }
      ],
      "key-spacing": [
        2,
        {
          "beforeColon": false,
          "afterColon": true,
          "mode": "strict"
        }
      ],
      "keyword-spacing": [
        2,
        {
          "before": true,
          "after": true,
          "overrides": {}
        }
      ],
      "line-comment-position": 0,
      "linebreak-style": 2,
      "lines-around-comment": [
        2,
        {
          "beforeBlockComment": true,
          "beforeLineComment": true,
          "allowBlockStart": true,
          "allowBlockEnd": true
        }
      ],
      "max-lines": [
        1,
        {
          "max": 450,
          "skipBlankLines": true,
          "skipComments": false
        }
      ],
      "max-nested-callbacks": [
        2,
        {
          "max": 2
        }
      ],
      "max-statements-per-line": [
        2,
        {
          "max": 1
        }
      ],
      "no-mixed-spaces-and-tabs": 2,
      "no-return-await": 2,
      "no-trailing-spaces": [
        2,
        {
          "skipBlankLines": false
        }
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "arrow-spacing": [
        2,
        {
          "before": true,
          "after": true
        }
      ],
      "no-unused-expressions": 2,
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        2,
        "last"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^7.2.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdoc": "^3.6.5",
    "react-dnd-test-backend": "^11.1.3",
    "react-dnd-test-utils": "^11.1.3",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^1.0.1",
    "typescript": "^3.9.7"
  },
  "proxy": "http://localhost:8080"
}
